    <link rel="stylesheet" href="/assets/css/checkout.css">

    <title>ALPHAPREF</title>
</head>

<body data-spy="scroll" data-target=".navbar" class="has-loading-screen" data-bg-parallax="scroll"
    data-bg-parallax-speed="3">
    <div class="ts-page-wrapper" id="page-top">
        <%- include("sections/header.ejs") %>
        <main id="ts-content">
            <%- include("sections/how-it-works.ejs") %>
            <%- include("sections/what-is.ejs") %>
            <%- include("sections/features.ejs") %>
            <%- include("sections/organize.ejs") %>
